[production]
resources.doctrineEntityManager.connection.driver    = "pdo_mysql"
resources.doctrineEntityManager.connection.host      = "<%= @db_host %>"
resources.doctrineEntityManager.connection.dbname    = "<%= @db_name %>"
resources.doctrineEntityManager.connection.user      = "<%= @db_user %>"
resources.doctrineEntityManager.connection.password  = "<%= @db_pass %>"
resources.doctrineEntityManager.key                  = "<%= @encryption_key %>" // A 32 character key used for encryption